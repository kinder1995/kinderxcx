/**app.wxss**/
@font-face {
    font-family: 'iconfont';
    src: url(data:application/font-woff2;charset=utf-8;base64,) format('woff2'),
        url(data:application/font-woff;charset=utf-8;base64,) format('woff');
    font-weight: normal;
    font-style: normal;
}

.iconfont{font-family:"iconfont" !important;font-size:16px;font-style:normal;-webkit-font-smoothing: antialiased;-moz-osx-font-smoothing: grayscale;}

.icon-riqi:before{content: "\e65d"; }
.icon-biaoqian:before{content: "\e62b"; }
.container{height: 100%;display: flex;flex-direction: column;align-items: center;justify-content: space-between;padding: 200rpx 0;box-sizing: border-box;}

page{background:#d9d9d9;padding-bottom:20rpx;}

.clear{clear:both;}

.duguo{font-size:36rpx;text-align:center;padding-top:40rpx;}

.date-box{text-align:center;margin-bottom:40rpx;}
.date-box .num,.date-box .text{display:inline-block;}
.date-box .num{font-size:36rpx;}
.date-box .text{font-size:32rpx;margin-right:10rpx;}

.list{padding:0 40rpx;margin-top:40rpx;}
.list .box{background:#fff;border-radius:10rpx;position:relative;box-shadow:0px 0px 30rpx rgba(0,0,0,0.15);padding-top:20rpx;overflow:hidden;}
.list .box .title{font-size:26rpx;border-left:10rpx solid #e84c3d;padding-left:20rpx;margin-bottom:20rpx;}
.list .box .ms{font-size:20rpx;padding:20rpx;background:#f4f4f4;border-top:1px solid #dee1e0;border-bottom:1px solid #dee1e0;line-height:42rpx;text-indent:2em;}
.list .box .bottom{padding:20rpx 20rpx;font-size:22rpx;}
.list .box .bottom .iconfont{font-size:26rpx;display:inline-block;vertical-align:middle;margin-top:-4rpx;margin-right:10rpx;}
.list .box .bottom .icon-biaoqian{font-size:34rpx;}
.list .box .bottom .data,.list .box .bottom .tag{color:#bfbfbf;font-size:12px;float:left;margin-right:18rpx;margin-top:4rpx;}

.list .box .bottom .read{float:right;color:#fff;border-radius:6rpx;background:#fb6362;text-align:center;box-shadow:0px 6rpx 0px #b83e3d;width:110rpx;height:40rpx;line-height:42rpx;}

.details{}
.details .box .title{height:auto;line-height:44rpx;}

.more{line-height:100rpx;text-align:center;color:#666;font-size:24rpx;}